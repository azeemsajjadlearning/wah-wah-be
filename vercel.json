{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],"functions": {
    "api/*": {
      "maxDuration": 10,
      "maxMemory": 128,
      "memory": 300,
      "maxSize": 10
    }
  }
}
